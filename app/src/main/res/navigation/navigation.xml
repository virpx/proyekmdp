<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.myapplication.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" /><action android:id="@+id/action_global_loginFragment" app:destination="@id/loginFragment"/>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.myapplication.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <action android:id="@+id/action_global_homeFragment" app:destination="@id/homeFragment" />
    <fragment
        android:id="@+id/dokterFragment"
        android:name="com.example.myapplication.DokterFragment"
        android:label="fragment_dokter"
        tools:layout="@layout/fragment_dokter" /><action android:id="@+id/action_global_dokterFragment" app:destination="@id/dokterFragment"/>
    <fragment
        android:id="@+id/userFragment"
        android:name="com.example.myapplication.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" />
</navigation>